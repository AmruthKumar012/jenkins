pipeline {
  agent any
  stages {
    stage('Parallel Stage') {
      parallel {
        stage('Job 1') {
          steps {
            sleep 10
            echo 'Running Job 1'
          }
        }
        stage('Job 2') {
          steps {
            sleep 5
            echo 'Running Job 2'
          }
        }
      }
    }
  }
}